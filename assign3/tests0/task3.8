(include "random.lib")
(randomSeed 106)
(define dq (deque))
(define (upback dq n)
    (define value (randomRange 10 100))
    (cond
        ((= n 0)
            ((dq'enqueueBack) value)
            )
        (else
            ((dq'enqueueBack) value)
            (upback dq (- n 1))
            )
        )
    )
(define (upfront dq n)
    (define value (randomRange 10 100))
    (cond
        ((= n 0)
            ((dq'enqueueFront) value)
            )
        ((< (dq'size) 10)
            ((dq'enqueueFront) value)
            (upfront dq (- n 1))
            )
        (else
            ((dq'enqueueIndex) (randomRange 2 10) value)
            (upfront dq (- n 1))
            )
        )
    )
(define (downback dq n)
    (cond
        ((or (= n 0) (< (dq'size) 10))
            ((dq'dequeueIndex) 0)
            )
        (else
            ((dq'dequeueIndex) (- (dq'size) (randomRange 2 10)))
            (downback dq (- n 1))
            )
        )
    )
(upback dq 2541)
(upfront dq 2598)
(downback dq 4950)
((dq'display)) (println)
